{
  "name": "models",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "dependsOn": ["generate-types"],
      "options": {
        "cwd": "libs/models",
        "command": "mkdir -p ../../dist/libs/models && cp prisma/* ../../dist/libs/models"
      }
    },
    "prisma": {
      "command": "prisma",
      "options": {
        "cwd": "libs/models"
      }
    },
    "migrate": {
      "command": "prisma migrate dev",
      "options": {
        "cwd": "libs/models"
      }
    },
    "generate-types": {
      "command": "prisma generate",
      "options": {
        "cwd": "libs/models"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/models/**/*.ts"]
      }
    }
  },
  "tags": []
}
